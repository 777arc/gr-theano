<?xml version="1.0"?>
<block>
  <name>Theano FFT</name>
  <key>theano_fft</key>
  <category>theano</category>
  <import>import grtheano</import>
  <make>grtheano.fft($N)</make>

  <param>
    <name>N</name>
    <key>N</key>
    <value>1024</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>

</block>
